{
	"app_id": "example-lifecycle",
	"display_name": "Example of App lifecycle event handling.",
	"homepage_url": "https://github.com/mattermost/mattermost-plugin-apps/tree/master/examples/go/lifecycle",
	"requested_permissions": [
		"act_as_bot"
	],
	"on_install": {
		"path": "/install",
		"expand":{
			"acting_user":"+id"
		}
	},
	"on_version_changed": {
		"path": "/version_changed",
		"expand":{
			"acting_user":"+id"
		}
	},
	"on_uninstall": {
		"path": "/uninstall",
		"expand":{
			"acting_user":"+id"
		}
	},
	"on_enable": {
		"path": "/enable",
		"expand":{
			"acting_user":"+id"
		}
	},
	"on_disable": {
		"path": "/disable",
		"expand":{
			"acting_user":"+id"
		}
	},
	"http": {
		"root_url": "http://localhost:8083",
		"expand":{
			"acting_user":"+id"
		}
	}
}