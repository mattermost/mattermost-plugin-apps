{
	"app_id": "hello-lifecycle",
	"display_name": "Hello, Lifecycle!",
	"homepage_url": "https://github.com/mattermost/mattermost-plugin-apps/tree/master/examples/go/hello-lifecycle",
	"requested_permissions": [
		"act_as_bot"
	],
	"on_install": {
		"path": "/install"
