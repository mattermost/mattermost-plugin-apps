{
	"app_id": "hello-lambda",
	"version": "demo",
	"display_name": "Hello, AWS Lambda!",
	"app_type": "aws_lambda",
	"homepage_url": "https://github.com/mattermost/mattermost-plugin-apps",
	"requested_permissions": [
		"act_as_bot"
	],
	"requested_locations": [
		"/command"
	],
	"aws_lambda": [
		{
			"path": "/",
			"name": "go-function",
			"handler": "hello-lambda",
			"runtime": "go1.x"
		}
	]
}
