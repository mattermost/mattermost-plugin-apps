{
	"app_id": "hello-lambda",
	"version": "demo",
	"display_name": "Hello, AWS Lambda!",
	"homepage_url": "https://github.com/mattermost/mattermost-plugin-apps",
	"requested_permissions": [
		"act_as_bot"
	],
	"requested_locations": [
		"/command"
	],
	"http": {
		"root_url": "http://localhost:8084"
	},
	"aws_lambda": {
		"functions": [
			{
				"path": "/",
				"name": "hello-lambda",
				"handler": "hello-lambda",
				"runtime": "go1.x"
			}
		]
	}
}